class Network

  constructor: (@q, @log, @rpc, @error_service) ->
    @log.info "---- Network API Constructor ----"

<% network_api['methods'].each do |m| %>
  # <%= m['description'] %>
  <%= m['method_name'].gsub('network_', '') %>: <%= js_method_params(m) %> ->
    @rpc.request('<%= m['method_name'] %>'<%= js_pass_params(m) %>).then (response) ->
      response.result
<% end %>


angular.module("app").service("NetworkAPI", ["$q", "$log", "RpcService", "ErrorService", Network])
